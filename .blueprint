models:
    Event: {  }
    Post: { post_category_id: foreign, user_id: foreign, thumbnail: string, title: string, slug: string, content: text, status: string, published_at: timestamp }
    Product: { category_id: foreign, thumbnail: string, name: string, slug: string, sku: string, price: bigInteger, size: string, description: 'text nullable', relationships: { hasMany: 'Cart, TransactionDetail' } }
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    Category: { name: string, slug: string, relationships: { hasMany: Product } }
    PostCategory: { name: string, slug: string, relationships: { hasMany: Post } }
    Slider: { thumbnail: string, tagline: string, title: string, content: text }
    Banner: { title: string, image: string }
    Cart: { product_id: foreign, qty: integer, price: bigInteger }
    Transaction: { transaction_code: string, user_id: foreign, total_amount: bigInteger, transaction_status: string, payment_status: string, payment_date: timestamp, order_date: timestamp, payment_method: string, relationships: { hasMany: TransactionDetail } }
    TransactionDetail: { transaction_id: foreign, product_id: foreign, qty: integer, price: bigInteger, subtotal: integer }
    ProductCategory: { name: string, slug: string, relationships: { hasMany: Product } }
